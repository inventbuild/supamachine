{
  "name": "@inventbuild/supamachine",
  "version": "0.3.3",
  "description": "Deterministic auth state machine for Supabase apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
