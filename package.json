    "publish": "npm run build && npm login && npm publish --access public"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "react": ">=18.0.0 <20"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
